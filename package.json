{
  "name": "jsonlintdotcom",
  "version": "0.0.1",
  "description": "The JSON Validator",
  "private": true,
  "scripts": {
      "test": "npm run lint && npm run stylelint",
      "bundle": "NODE_ENV=production webpack",
      "develop": "NODE_ENV=development webpack-dev-server --port 8101",
      "check-updates": "ncu",
      "upgrade": "npm run check-updates -- --upgradeAll && npm install",
      "lint": "eslint . --ext .js --cache --ignore-pattern '**/node_modules/**' --ignore-pattern '**/dist/**'",
      "deploy": "pm2 deploy ecosystem.json production",
      "stylelint": "stylelint css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circlecell/jsonlintdotcom.git"
  },
  "keywords": [
    "jsonlint",
    "lint",
    "json"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/circlecell/jsonlintdotcom/issues"
  },
  "homepage": "https://github.com/circlecell/jsonlintdotcom#readme",
  "devDependencies": {
      "babel-cli": "^6.11.4",
      "babel-core": "^6.11.4",
      "babel-eslint": "^6.1.2",
      "babel-loader": "^6.2.4",
      "babel-preset-es2015": "^6.9.0",
      "babel-preset-stage-1": "^6.5.0",
      "babel-register": "^6.11.6",
      "copy-webpack-plugin": "^3.0.1",
      "css-loader": "^0.23.1",
      "eslint": "^3.2.2",
      "eslint-config-airbnb": "^10.0.0",
      "eslint-loader": "^1.5.0",
      "eslint-plugin-import": "^1.12.0",
      "eslint-plugin-jsx-a11y": "^2.0.1",
      "eslint-plugin-react": "^6.0.0",
      "npm-check-updates": "^2.8.0",
      "open-browser-webpack-plugin": "0.0.2",
      "script-loader": "^0.7.0",
      "style-loader": "^0.13.1",
      "stylelint": "^7.1.0",
      "stylelint-config-standard": "^12.0.0",
      "webpack": "^1.13.1",
      "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "codemirror": "^5.8.0",
    "js-beautify": "^1.5.10",
    "jsonlint": "^1.6.2"
  }
}
