{
  "name": "jsonlintdotcom",
  "version": "0.0.1",
  "description": "The JSON Validator",
  "private": true,
  "scripts": {
    "test": "npm run lint",
    "build": "npm run clean && NODE_ENV=production webpack",
    "develop": "NODE_ENV=development webpack-dev-server --port 8106",
    "check-updates": "ncu",
    "clean": "rimraf dist",
    "upgrade": "npm run check-updates -- --upgradeAll && npm install",
    "lint": "eslint . --ext .js --cache --ignore-pattern '**/node_modules/**' --ignore-pattern '**/dist/**'",
    "deploy": "pm2 deploy ecosystem.json production",
    "stylelint": "stylelint static/css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circlecell/jsonlintdotcom.git"
  },
  "keywords": [
    "jsonlint",
    "lint",
    "json"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/circlecell/jsonlintdotcom/issues"
  },
  "homepage": "https://github.com/circlecell/jsonlintdotcom#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "exports-loader": "^0.6.3",
    "html-webpack-plugin": "^2.24.1",
    "napa": "^2.3.0",
    "npm-check-updates": "^2.8.6",
    "open-browser-webpack-plugin": "0.0.3",
    "optimist": "^0.6.1",
    "rimraf": "^2.5.4",
    "script-loader": "^0.7.0",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-split-by-path": "^0.1.0"
  },
  "dependencies": {
    "balajs": "^0.1.9",
    "codemirror": "^5.21.0",
    "js-beautify": "^1.6.4",
    "jsonlint-mod": "^1.7.0",
    "jsonminify": "^0.4.1"
  }
}
